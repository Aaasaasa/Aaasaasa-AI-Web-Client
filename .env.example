# Use 'development' during development, 'production' for build
NODE_ENV=development
DEV_SERVER_URL=http://localhost:3000
AAASAASA_BASE=

NUXT_BASE=/
NUXT_PUBLIC_APP_BASE_URL=/                 # npr. / ili /app/
NUXT_PUBLIC_DUCKDB_BASE=/duckdb            # public/duckdb/*
NUXT_PUBLIC_API_BASE=/api
NUXT_PUBLIC_WS_BASE=ws://localhost:3000

# Dev allowlist: renderer + Qdrant + tipični AI servisi/CDN-ovi
NUXT_PUBLIC_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:6333,https://api.openai.com,https://huggingface.co,https://*.hf.space,https://*.s3.amazonaws.com,https://*.r2.cloudflarestorage.com,https://cdn.jsdelivr.net,https://unpkg.com,https://plausible.io,https://*.ingest.sentry.io,https://auth.your-domain.tld

# AI (prazno u dev-u, koristi lokalne modele po želji)
NUXT_OPENAI_API_KEY=                       # ako koristiš OpenAI
NUXT_OPENAI_BASE_URL=https://api.openai.com/v1
NUXT_OLLAMA_BASE_URL=http://localhost:11434
NUXT_HF_API_KEY=
NUXT_HF_INFERENCE_URL=                     # npr. https://api-inference.huggingface.co/models/...

# Vector/DB/cache (lokalno)
NUXT_QDRANT_URL=http://localhost:6333
NUXT_QDRANT_API_KEY=
NUXT_DB_URL=postgres://user:pass@localhost:5432/app
NUXT_REDIS_URL=redis://localhost:6379

# Ostalo (prazno u dev-u)
NUXT_S3_ENDPOINT=
NUXT_S3_REGION=
NUXT_S3_ACCESS_KEY_ID=
NUXT_S3_SECRET_ACCESS_KEY=
NUXT_S3_BUCKET=
NUXT_SENTRY_DSN=
NUXT_PUBLIC_PLAUSIBLE_DOMAIN=your-domain.tld
