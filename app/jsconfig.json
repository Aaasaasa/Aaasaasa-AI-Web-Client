{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "ES2022",
    "jsx": "preserve",
    "allowJs": true,
    "checkJs": false,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "#imports": [".nuxt/imports"]
    }
  },
  "include": ["nuxt.d.ts", "**/*.js", "**/*.ts", "**/*.vue"],
  "exclude": ["node_modules"]
}
